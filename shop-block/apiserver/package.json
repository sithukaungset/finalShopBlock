{
    "name": "fabcar",
    "version": "1.0.0",
    "description": "FabCar application implemented in JavaScript",
    "engines": {
        "node": ">=8",
        "npm": ">=5"
    },
    "scripts": {
        "lint": "eslint .",
        "pretest": "npm run lint",
        "test": "nyc mocha --recursive"
    },
    "engineStrict": true,
    "author": "Hyperledger",
    "license": "Apache-2.0",
    "dependencies": {
        "body-parser": "^1.19.0",
        "bootstrap": "^5.1.3",
        "ejs": "^3.1.6",
        "express": "^4.17.1",
        "fabric-ca-client": "^2.2.4",
        "fabric-network": "^2.2.4",
        "jade": "^1.11.0",
        "jquery": "^3.6.0",
        "prompt": "^1.2.1",
        "prompt-sync": "^4.2.0",
        "pug": "^3.0.2"
    },
    "devDependencies": {
        "chai": "^4.2.0",
        "css-loader": "^6.7.1",
        "eslint": "^5.9.0",
        "mocha": "^5.2.0",
        "mocha-jsdom": "^2.0.0",
        "nyc": "^14.1.1",
        "sinon": "^7.1.1",
        "sinon-chai": "^3.3.0",
        "style-loader": "^3.3.1"
    },
    "nyc": {
        "exclude": [
            "coverage/**",
            "test/**"
        ],
        "reporter": [
            "text-summary",
            "html"
        ],
        "all": true,
        "check-coverage": true,
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
    }
}
